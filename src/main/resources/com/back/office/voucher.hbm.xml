<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.Voucher" table="voucher_details" catalog="">
        <id name="voucherId" type="java.lang.Integer">
            <column name="voucherId" />
            <generator class="identity" />
        </id>
        <property name="voucherName" type="java.lang.String">
            <column name="voucherName" length="50" />
        </property>
        <property name="voucherType" type="java.lang.String">
            <column name="voucherType" length="50" />
        </property>
        <property name="discount" type="java.lang.String">
            <column name="discount" length="200" />
        </property>
        <property name="activateDate" type="java.lang.String">
            <column name="activateDate" length="50"/>
        </property>
        <property name="endDate" type="java.lang.String">
            <column name="endDate"  length="50"/>
        </property>
        <property name="amount" type="java.lang.Float">
            <column name="amount" />
        </property>
    </class>
</hibernate-mapping>