<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.EquipmentMaster" table="equipment_master" catalog="">
        <id name="equipmentMasterId" type="java.lang.Integer">
            <column name="eqMasterId" />
            <generator class="identity" />
        </id>
        <property name="equipmentId" type="java.lang.String">
            <column name="equipmentId"/>
        </property>
        <property name="type" type="java.lang.String">
            <column name="type"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="status"/>
        </property>
        <property name="recordStatus" type="java.lang.Integer">
            <column name="recordStatus" />
        </property>
        <property name="flightNumber" type="java.lang.String">
            <column name="flightNumber"  />
        </property>
        <property name="lastUsedDate" type="java.util.Date">
            <column name="lastUsedDate" length="50" />
        </property>

    </class>
</hibernate-mapping>