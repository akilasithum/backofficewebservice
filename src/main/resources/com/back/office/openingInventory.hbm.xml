<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.OpeningInventory" table="opening_inventory" catalog="">
        <id name="inventoryItemId" type="java.lang.Integer">
            <column name="inventoryItemId" />
            <generator class="identity" />
        </id>
        <property name="itemId" type="java.lang.String">
            <column name="itemId" length="50" />
        </property>
        <property name="sifNo" type="java.lang.String">
            <column name="sifNo"  length="50"/>
        </property>
        <property name="quantityInt" type="java.lang.Integer">
            <column name="quantity"  />
        </property>
        <property name="cartNo" type="java.lang.String">
            <column name="cartNo"  length="50"/>
        </property>
        <property name="drawer" type="java.lang.String">
            <column name="drawer" length="50" />
        </property>
    </class>
</hibernate-mapping>