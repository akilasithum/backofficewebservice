<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.PreOrder" table="pre_orders" catalog="">
        <id name="preOrderId" type="java.lang.Integer">
            <column name="preOrderId" />
            <generator class="identity" />
        </id>
        <property name="PNR" type="java.lang.String">
            <column name="PNR" length="10"/>
        </property>
        <property name="customerName" type="java.lang.String">
            <column name="customerName" length="50" />
        </property>
        <property name="serviceType" type="java.lang.String">
            <column name="serviceType" length="20" />
        </property>
    </class>
</hibernate-mapping>