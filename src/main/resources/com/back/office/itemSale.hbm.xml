<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.ItemSale" table="pos_item_sale_details" catalog="">
        <id name="saleOrderId" type="java.lang.Integer">
            <column name="saleOrderId" />
            <generator class="identity" />
        </id>
        <property name="orderId" type="java.lang.String">
            <column name="orderId" length="50"/>
        </property>
        <property name="itemIdInt" type="java.lang.Integer">
            <column name="itemId"/>
        </property>
        <property name="quantityInt" type="java.lang.Integer">
            <column name="quantity" />
        </property>
        <property name="priceFloat" type="java.lang.Float">
            <column name="price" />
        </property>
    </class>
</hibernate-mapping>