<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.back.office.ws.entity.BondMessage" table="bond_messages" catalog="">
        <id name="bondMessageId" type="java.lang.Integer">
            <column name="bondMessageId" />
            <generator class="identity" />
        </id>
        <property name="flightNo" type="java.lang.String">
            <column name="flightNo"  />
        </property>
        <property name="messageBody" type="java.lang.String">
            <column name="messageBody" />
        </property>
        <property name="flightDate" type="java.util.Date">
            <column name="flightDate"  />
        </property>
    </class>
</hibernate-mapping>
